        .mcontainer {
            max-width: 800px;
            margin: 200px auto;
            padding: 20px;
            background-color: #fff;
            border-radius: 8px;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
            position: relative;
        }

        .mheader {
            font-size: 28px;
            font-weight: bold;
            margin-bottom: 20px;
            color: #444;
        }

        .mback {
            border: 1px solid #b07b36;
            color: #b07b36;
            background-color: #fff;
            border-radius: 4px;
            cursor: pointer;
            text-align: center;
            position: absolute;
            left: 94.5%;
            top: 0%;
            transform: translateX(-50%);
            padding: 10px 20px;
        }

        .mback:hover {
            background-color: #b07b36;
            color: white;
        }

        .msection {
            margin-bottom: 20px;
        }

        .msection-title {
            font-size: 18px;
            font-weight: bold;
            margin-bottom: 8px;
            color: #666;
        }

        .morder-info,
        .muser-info,
        .mpayment-info,
        .morder-status {
            margin-bottom: 15px;
            padding: 15px;
            border-radius: 5px;
            background-color: #f7f7f7;
            font-size: 16px;
        }

        .mmorder-info span,
        .muser-info span,
        .mpayment-info span,
        .morder-status span {
            font-weight: bold;
            color: #333;
        }

        .mproducts-table {
            width: 100%;
            border-collapse: collapse;
            margin-top: 10px;
        }

        .mproducts-table th,
        .products-table td {
            padding: 10px;
            text-align: left;
            border-bottom: 1px solid #ddd;
        }

        .mproducts-table th {
            background-color: #f0f0f0;
            color: #333;
            font-weight: bold;
        }

        .mstatus {
            font-weight: bold;
            color: #333;
        }

        .mstatus.Pending {
            color: #e67e22;
        }

        .mstatus.Shipped {
            color: #3498db;
        }

        .mstatus.Delivered {
            color: #27ae60;
        }

        .mstatus.Cancelled {
            color: #e74c3c;
        }
    </style>
