						{{#if (gt currentPage 1)}}
						<a href="?page={{subtract currentPage 1}}{{#if selectedCategory}}&category={{selectedCategory}}{{/if}}{{#if sort}}&sort={{sort}}{{/if}}{{#if minPrice}}&minPrice={{minPrice}}{{/if}}{{#if maxPrice}}&maxPrice={{maxPrice}}{{/if}}"
							class="prev-arrow">
							<i class="fa fa-long-arrow-left" aria-hidden="true"></i>
						</a>
						{{/if}}

						{{#each (range 1 totalPages)}}
						<a href="?page={{this}}{{#if selectedCategory}}&category={{../selectedCategory}}{{/if}}{{#if ../sort}}&sort={{../sort}}{{/if}}{{#if ../minPrice}}&minPrice={{../minPrice}}{{/if}}{{#if ../maxPrice}}&maxPrice={{../maxPrice}}{{/if}}"
							class="{{#if (eq this ../currentPage)}}active{{/if}}">
							{{this}}
						</a>
						{{/each}}

						{{#if (lt currentPage totalPages)}}
						<a href="?page={{add currentPage 1}}{{#if selectedCategory}}&category={{selectedCategory}}{{/if}}{{#if sort}}&sort={{sort}}{{/if}}{{#if minPrice}}&minPrice={{minPrice}}{{/if}}{{#if maxPrice}}&maxPrice={{maxPrice}}{{/if}}"
							class="next-arrow">
							<i class="fa fa-long-arrow-right" aria-hidden="true"></i>
						</a>
						{{/if}}
					</div>
				</div>
				<!-- End Filter Bar -->
