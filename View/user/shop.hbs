					<div class="filter-summary">
					</div>
					<div class="top-filter-head">
						<p>Product Filters</p>
					</div>
					<form id="filterForm" action="/shop" method="get">
						<!-- Category Filter -->
						<div class="common-filter">
							<div class="head">Category</div>
							<ul>
								{{#each categories}}
								<li class="filter-list">
									<input class="pixel-radio" type="radio" id="{{this._id}}" name="category"
										value="{{this.name}}" {{#if (eq this.name ../selectedCategory)}}
										checked="checked" {{/if}}>
									<label for="{{this._id}}">
										{{this.name}}
									</label>
								</li>
								{{/each}}
							</ul>
						</div>

						<!-- Sort Filter -->
						<div class="common-filter">
							<div class="head">Sort by</div>
							<ul>
								<li class="filter-list">
									<input class="pixel-radio" type="radio" value="az" name="sort" {{#if (eq
										sortOption 'az' )}} checked {{/if}}>
									A - Z
								</li>
								<li class="filter-list">
									<input class="pixel-radio" type="radio" value="za" name="sort" {{#if (eq
										sortOption 'za' )}} checked {{/if}}>
									Z - A
								</li>
								<li class="filter-list">
									<input class="pixel-radio" type="radio" value="new-arrivals" name="sort" {{#if (eq
										sortOption 'new-arrivals' )}} checked {{/if}}>
									New Arrivals
								</li>
								<li class="filter-list">
									<input class="pixel-radio" type="radio" value="price-desc" name="sort" {{#if (eq
										sortOption 'price-desc' )}} checked {{/if}}>
									Price High to Low
								</li>
								<li class="filter-list">
									<input class="pixel-radio" type="radio" value="price-asc" name="sort" {{#if (eq
										sortOption 'price-asc' )}} checked {{/if}}>
									Price Low to High
								</li>
							</ul>
						</div>
						<!-- Price Filter -->
						<div class="common-filter">
							<div class="head1">Price</div>
							<input type="number" name="minPrice" placeholder="Min Price" value="{{minPrice}}" />
							<input type="number" name="maxPrice" placeholder="Max Price" value="{{maxPrice}}" />
						</div>
						<button class="buttonApply" type="submit">Apply Filters</button>
						<a href="/shop" class="buttonReset">Reset Filters</a>
					</form>
				</div>
			</div>
